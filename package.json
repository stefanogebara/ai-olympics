{
  "name": "ai-olympics",
  "version": "0.1.0",
  "description": "AI Agent Olympics - Competitive AI entertainment platform where agents compete in real-world internet tasks",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "orchestrator": "tsx watch src/orchestrator/index.ts",
    "stream": "tsx watch src/streaming/index.ts",
    "api": "tsx watch src/api/index.ts",
    "competition": "tsx src/orchestrator/run-competition.ts",
    "tournament": "tsx src/orchestrator/run-tournament.ts",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "dev:all": "concurrently \"npm run api\" \"npm run frontend:dev\"",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "docker:build": "docker build -t ai-olympics-agent -f infrastructure/docker/Dockerfile.agent .",
    "docker:sandbox": "docker-compose -f infrastructure/docker/docker-compose.sandbox.yml up",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/generative-ai": "^0.21.0",
    "@polymarket/clob-client": "^5.2.1",
    "@sentry/node": "^10.38.0",
    "@supabase/supabase-js": "^2.94.1",
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "date-fns": "^4.1.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "nanoid": "^5.0.9",
    "obs-websocket-js": "^5.0.6",
    "openai": "^4.77.0",
    "ora": "^8.1.0",
    "p-queue": "^8.0.1",
    "playwright": "^1.50.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^20.3.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "yamljs": "^0.3.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/dockerode": "^3.3.31",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^25.2.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.5.13",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "agents",
    "competition",
    "olympics",
    "claude",
    "gpt-4",
    "gemini",
    "playwright",
    "streaming"
  ],
  "lint-staged": {
    "src/**/*.ts": "bash -c 'npx tsc --noEmit --skipLibCheck'",
    "frontend/src/**/*.{ts,tsx}": "bash -c 'cd frontend && npx tsc --noEmit --skipLibCheck'"
  },
  "author": "Stefan Gebara",
  "license": "MIT"
}
